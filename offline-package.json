{
  "name": "gauge-js",
  "version": "2.3.8",
  "description": "JavaScript runner for Gauge",
  "main": "index.js",
  "config": {
    "hasPureJsGrpc": true
  },
  "scripts": {
    "lint": "eslint index.js src/ skel/ test/ scripts/",
