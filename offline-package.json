  "homepage": "https://github.com/getgauge/gauge-js",
  "dependencies": {
    "console-stamp": "^0.2.9",
    "escodegen": "^1.12.0",
    "esprima": "^4.0.1",
    "estraverse": "^4.3.0",
    "klaw-sync": "^3.0.2",
    "protobufjs": "^6.8.8",
    "q": "^1.5.0",
    "@grpc/grpc-js": "^0.5.3",
    "@grpc/proto-loader": "^0.5.2"
  },
  "devDependencies": {
    "archiver": "^3.1.1",
    "chai": "^4.2.0",
    "check-if-windows": "^1.0.0",
    "eslint": "^6.4.0",
