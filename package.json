    "protobufjs": "~6.8.0",
    "q": "^1.5.0"
  },
  "devDependencies": {
    "archiver": "^2.0.3",
    "chai": "^4.1.2",
    "check-if-windows": "^1.0.0",
    "fs-extra": "^4.0.2",
    "jscs": "^3.0.7",
